<launch>

    <rosparam command="load" file="$(find araig_interpreters)/tests/config/test_goal_interpreter_action.yaml"/>

    <test test-name="test_goal_interpreter" pkg="araig_interpreters" type="test_goal_interpreter_action.py">
    </test>

    <node pkg="araig_interpreters" type="goal_interpreter" name="goal_interpreter" output="screen">
        <remap from="/start" to="/test/start"/>
        <remap from="/goal" to="/test/goal"/>    
    </node>

</launch>